# SAP AI Core Authentication URL
AI_CORE_AUTH_URL=https://subdomainaicore.authentication.eu10.hana.ondemand.com/oauth/token

# SAP AI Core Client ID (obtained from SAP BTP cockpit)
AI_CORE_CLIENT_ID=your-client-id-here

# SAP AI Core Client Secret (obtained from SAP BTP cockpit)
AI_CORE_CLIENT_SECRET=your-client-secret-here

# SAP AI Core Deployment URL (Claude 3.5 deployment)
AI_CORE_DEPLOYMENT_URL=https://api.ai.prod.eu-central-1.aws.ml.hana.ondemand.com/v2/inference/deployments/your-deployment-id

# Resource group (default is usually fine)
AI_CORE_RESOURCE_GROUP=default

# Server port (optional, defaults to 4004)
PORT=4004